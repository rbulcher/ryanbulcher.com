<!DOCTYPE HTML>
<html>
<head>
	{{>head}}
</head>

<body>
	<div id="colorlib-page">
		<div class="container-wrap" id="vertex-container">
		<a href="#" class="js-colorlib-nav-toggle colorlib-nav-toggle" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar"><i></i></a>

        {{>sidebar-home}}

		<div id="colorlib-main">
			<section id="home" class="js-fullheight" data-section="home">
				<div class="flexslider js-fullheight">
					<ul class="slides">
                        <li style="background-image: url({{{page.home.img_bg}}}=s2040);">
                            <div class="overlay"></div>
                            <div class="container-fluid">
                                <div class="row">
                                    <div class="col-md-6 col-md-offset-3 col-md-pull-3 col-sm-12 col-xs-12 js-fullheight slider-text">
                                        <div class="slider-text-inner">
                                            <div class="desc">
                                                <h1>{{page.home.title}}<br>{{page.home.title2}}</h1>
                                                <h2>{{page.home.subtitle}}</h2>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </li>
				  	</ul>
			  	</div>
			</section>

            {{#page.about.show_or_hide}}
			<section class="colorlib-about" data-section="about" id="about">
				<div class="colorlib-narrow-content">
					<div class="row">
						<div class="col-md-12">
							<div class="row row-bottom-padded-sm animate-box" data-animate-effect="fadeInLeft">
								<div class="col-md-12">
									<div class="about-desc">
										<span class="heading-meta">{{page.about.subheader}}</span>
										<h2 class="colorlib-heading">{{page.about.header}}</h2>
										<p>{{page.about.description}}</p>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-md-3 animate-box" data-animate-effect="fadeInLeft">
									<div class="services" style="border-bottom: 2px solid {{page.about.lists.list1.color}};">
										<span class="icon2"><img src="{{{page.about.lists.list1.img_list}}}"/></span>
										<h3>{{page.about.lists.list1.text}}</h3>
									</div>
								</div>
								<div class="col-md-3 animate-box" data-animate-effect="fadeInRight">
									<div class="services" style="border-bottom: 2px solid {{page.about.lists.list2.color}};">
										<span class="icon2"><img src="{{{page.about.lists.list2.img_list}}}"/></span>
										<h3>{{page.about.lists.list2.text}}</h3>
									</div>
								</div>
								<div class="col-md-3 animate-box" data-animate-effect="fadeInTop">
									<div class="services" style="border-bottom: 2px solid {{page.about.lists.list3.color}};">
										<span class="icon2"><img src="{{{page.about.lists.list3.img_list}}}"/></span>
										<h3>{{page.about.lists.list3.text}}</h3>
									</div>
								</div>
								<div class="col-md-3 animate-box" data-animate-effect="fadeInBottom">
									<div class="services" style="border-bottom: 2px solid {{page.about.lists.list4.color}};">
										<span class="icon2"><img src="{{{page.about.lists.list4.img_list}}}"/></span>
										<h3>{{page.about.lists.list4.text}}</h3>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</section>
            {{/page.about.show_or_hide}}

            {{#page.services.show_or_hide}}
			<section class="colorlib-services" data-section="services" id="services">
				<div class="colorlib-narrow-content">
					<div class="row">
						<div class="col-md-6 col-md-offset-3 col-md-pull-3 animate-box" data-animate-effect="fadeInLeft">
							<span class="heading-meta">{{page.services.subheader}}</span>
							<h2 class="colorlib-heading">{{page.services.header}}</h2>
						</div>
					</div>
					<div class="row row-pt-md">
                        {{#services}}
						<div class="col-md-4 text-center animate-box">
							<div class="services color-1">
								<span class="icon">
									<img src="{{{image}}}"/>
								</span>
								<div class="desc">
									<h3>{{name}}</h3>
									<p>{{description}}</p>
								</div>
							</div>
						</div>
                        {{/services}}
					</div>
				</div>
			</section>
			{{/page.services.show_or_hide}}

            {{#page.skills.show_or_hide}}
			<section class="colorlib-skills" data-section="skills" id="skills">
				<div class="colorlib-narrow-content">
					<div class="row">
						<div class="col-md-6 col-md-offset-3 col-md-pull-3 animate-box" data-animate-effect="fadeInLeft">
                            <span class="heading-meta">{{page.skills.subheader}}</span>
							<h2 class="colorlib-heading animate-box">{{page.skills.header}}</h2>
						</div>
					</div>
					<div class="row">
						<div class="col-md-12 animate-box" data-animate-effect="fadeInLeft">
							<p>{{page.skills.description}}</p>
                        </div>
						<div id="skills-content"></div>
					</div>
				</div>
			</section>
            {{/page.skills.show_or_hide}}

            {{#page.education.show_or_hide}}
			<section class="colorlib-education" data-section="education" id="education">
				<div class="colorlib-narrow-content">
					<div class="row">
						<div class="col-md-6 col-md-offset-3 col-md-pull-3 animate-box" data-animate-effect="fadeInLeft">
							<span class="heading-meta">{{page.education.subheader}}</span>
							<h2 class="colorlib-heading animate-box">{{page.education.header}}</h2>
						</div>
					</div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="timeline-centered">
                                {{#schools}}
                                <article class="timeline-entry animate-box" data-animate-effect="fadeInLeft">
                                <div class="timeline-entry-inner">
                                    <div class="timeline-icon color-1">
                                        <i class="icon-book2"></i>
                                    </div>
                                    <div class="timeline-label">
                                        <h2 style="margin-top:16px">{{name}} <span>{{dateRange}}</span></h2>
                                        <p>{{description}}</p>
                                    </div>
                                </div>
                                </article>
                                {{/schools}}
                                <article class="timeline-entry begin animate-box" data-animate-effect="fadeInBottom">
                                    <div class="timeline-entry-inner">
                                    <div class="timeline-icon color-none">
                                    </div>
                                    </div>
                                </article>
                            </div>
                        </div>
                    </div>
				</div>
			</section>
            {{/page.education.show_or_hide}}

            {{#page.experience.show_or_hide}}
			<section class="colorlib-experience" data-section="experience" id="experience">
				<div class="colorlib-narrow-content">
					<div class="row">
						<div class="col-md-6 col-md-offset-3 col-md-pull-3 animate-box" data-animate-effect="fadeInLeft">
							<span class="heading-meta">{{page.experience.subheader}}</span>
							<h2 class="colorlib-heading animate-box">{{page.experience.header}}</h2>
						</div>
					</div>
					<div class="row">
						<div class="col-md-12">
				         <div class="timeline-centered">
                            {{#jobs}}
					         <article class="timeline-entry animate-box" data-animate-effect="fadeInLeft">
					            <div class="timeline-entry-inner">
					               <div class="timeline-icon color-1">
					                  <i class="icon-pen2"></i>
					               </div>
					               <div class="timeline-label">
					                  <h2>{{position}} <span>{{start}}-{{end}}</span></h2>
					                  <p>{{description}}</p>
					               </div>
					            </div>
					         </article>
                            {{/jobs}}
					         <article class="timeline-entry begin animate-box" data-animate-effect="fadeInBottom">
					            <div class="timeline-entry-inner">
					               <div class="timeline-icon color-none">
					               </div>
					            </div>
					         </article>
					      </div>
					   </div>
				   </div>
				</div>
			</section>
            {{/page.experience.show_or_hide}}

            {{#page.project.show_or_hide}}
			<section class="colorlib-work" data-section="work" id="project">
				<div class="colorlib-narrow-content">
					<div class="row">
						<div class="col-md-6 col-md-offset-3 col-md-pull-3 animate-box" data-animate-effect="fadeInLeft">
							<span class="heading-meta">{{page.project.subheader}}</span>
							<h2 class="colorlib-heading animate-box">{{page.project.header}}</h2>
						</div>
					</div>
					<div class="row">
                        {{#projects}}
						<div class="col-md-6 animate-box" data-animate-effect="fadeInLeft">
							<div class="project" style="background-image: url({{{image}}});">
								<div class="desc">
									<div class="con">
										<h3>{{name}}</h3>
										<span>{{description}}</span>
										<p class="icon">
											<span><a href="{{{link}}}" target="_blank"><i class="icon-link2"></i></a></span>
										</p>
									</div>
								</div>
							</div>
						</div>
                        {{/projects}}
					</div>
				</div>
			</section>
            {{/page.project.show_or_hide}}

            {{#page.blog.show_or_hide}}
			<section class="colorlib-blog" data-section="blog" id="blog">
				<div class="colorlib-narrow-content">
					<div class="row">
						<div class="col-md-6 col-md-offset-3 col-md-pull-3 animate-box" data-animate-effect="fadeInLeft">
							<span class="heading-meta">{{page.blog.subheader}}</span>
							<h2 class="colorlib-heading">{{page.blog.header}}</h2>
						</div>
					</div>
					<div class="row">
                        {{#posts}}
						<div class="col-md-4 col-sm-6 animate-box" data-animate-effect="fadeInLeft">
							<div class="blog-entry">
								<a href="/post/{{{slug}}}" class="blog-img"><img src="{{{image}}}" class="img-responsive" alt="{{global.site_name}}"></a>
								<div class="desc">
									<span><small>{{dateString}}</small></span>
									<h3><a href="/post/{{{slug}}}">{{title}}</a></h3>
									<p>{{preview}}</p>
								</div>
							</div>
						</div>
                        {{/posts}}
					</div>
				</div>
			</section>
            {{/page.blog.show_or_hide}}

            {{#page.contact.show_or_hide}}
			<section class="colorlib-contact" data-section="contact" id="contact">
				<div class="colorlib-narrow-content">
					<div class="row">
						<div class="col-md-6 col-md-offset-3 col-md-pull-3 animate-box" data-animate-effect="fadeInLeft">
							<span class="heading-meta">{{page.contact.subheader}}</span>
							<h2 class="colorlib-heading">{{page.contact.header}}</h2>
						</div>
					</div>
					<div class="row">
						<div class="col-md-7 col-md-push-1">
							<div class="row">
								<div class="col-md-10 col-md-pull-1 animate-box" data-animate-effect="fadeInRight">
									<form  action="" method="post">
										<div class="form-group">
											<input type="text" id="edd-user-name" class="form-control" placeholder="Name">
										</div>
										<div class="form-group">
											<input type="text" id="edd-user-email" class="form-control" placeholder="Email">
										</div>
										<div class="form-group">
											<input type="text" id="edd-user-subject" class="form-control" placeholder="Subject">
										</div>
										<div class="form-group">
											<textarea name="" id="edd-user-message" cols="30" rows="7" class="form-control" placeholder="Message"></textarea>
										</div>
										<div class="form-group">
											<input type="submit" id="btn-register" class="btn btn-primary btn-send-message" value="Send Message">
										</div>
									</form>
								</div>
							</div>
						</div>
                        <div class="col-md-5 animate-box" data-animate-effect="fadeInLeft">
							<p><img src="{{{page.contact.img_section}}}"/></p>
						</div>
					</div>
				</div>
			</section>
            {{/page.contact.show_or_hide}}

		</div><!-- end:colorlib-main -->
	</div><!-- end:container-wrap -->
	</div><!-- end:colorlib-page -->

    <script>window.__PRELOADED__ = {{{preloaded}}}</script>
	{{>scripts}}

	<script type="text/javascript" src="{{{cdn}}}/js/skills.js"></script>
</body>
</html>
